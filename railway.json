{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "gunicorn s2u_project.wsgi:application --bind 0.0.0.0:$PORT --timeout 3600 --graceful-timeout 30 --keep-alive 5",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
